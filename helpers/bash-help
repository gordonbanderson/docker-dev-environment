
#Docker dev environment aliases
# Get a bash shell on a docker terminal
dt() {
    docker exec -ti $1 /bin/bash
}

dpsql() {
    psql -U postgres -h localhost -p 54321 -v PROMPT1='%[%033[1m%]%M %n@%/%R%[%033[0m%]%# '
}

fksql() {
    psql -U postgres -h localhost -p 54321 foodkitapi
}

dl() {
    docker logs -f $1
}



